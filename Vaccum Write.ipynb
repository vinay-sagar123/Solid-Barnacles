   "source": [
    "# capturing Video from Webcam \n",
    "cap = cv2.VideoCapture(0)\n",
    "def text23(obj):\n",
    "    if obj:\n",
    "        hand = obj\n",
    "        x1 = hand[0].landmark[4].x\n",
    "        y1 = hand[0].landmark[4].y\n",
    "        x2 = hand[0].landmark[8].x\n",
    "        y2 = hand[0].landmark[8].y\n",
    "        p = [x1,y1]\n",
    "        q = [x2,y2]\n",
    "        dis = math.dist(p,q)\n",
    "        dis = dis * 10 \n",
    "        dis = round(dis)\n",
    "        return dis\n",
    "\n",
    "\n",
    "def drawline(obj,img):\n",
    "    hand = obj\n",
    "    x1 = hand[0].landmark[4].x\n",
    "    y1 = hand[0].landmark[4].y\n",
    "    x2 = hand[0].landmark[8].x\n",
    "    y2 = hand[0].landmark[8].y\n",
    "    cv2.line(img,(x1,y1),(x2,y2),(0,255,0),2)\n",
    "    \n",
    "    \n",
    "\n",
    "colors = [(255,0,0),(0,255,0),(0,0,0),(255,0,255),(218, 247, 166)]\n",
    "\n",
    "\n",
    "points = [deque()]\n",
    "blue_queue = [deque()]\n",
    "green_queue = [deque()]\n",
    "black_queue = [deque()]\n",
    "pink_queue = [deque()]\n",
    "cream_queue = [deque()]\n",
    "kernel = np.ones((5,5),np.uint8)\n",
    "\n",
    "blue_index = 0\n",
    "green_index = 0\n",
    "black_index = 0\n",
    "pink_index = 0\n",
    "cream_index = 0\n",
    "c = 0\n",
    "\n",
    "with mp_hands.Hands(min_detection_confidence=0.8, min_tracking_confidence=0.5) as hands:\n",
    "    while cap.isOpened():\n",
    "        ret, image = cap.read()\n",
    "        image = cv2.flip(image, 1)    \n",
    "        image = imutils.resize(image, width=1000)\n",
    "        image = cv2.putText(image, \"Clear All\", (53,45),cv2.FONT_HERSHEY_SIMPLEX,0.6,(0, 0, 0),1,cv2.LINE_AA)\n",
    "        image = cv2.rectangle(image, (40,10), (140,70), colors[2], 3)\n",
    "        image = cv2.rectangle(image, (160,10), (255,70), colors[2], 3)\n",
    "        image = cv2.rectangle(image, (275,10), (370,70), colors[2], 3)\n",
    "        image = cv2.rectangle(image, (390,10), (485,70), colors[2], 3)\n",
    "        image = cv2.rectangle(image, (505,10), (600,70), colors[2], 3)\n",
    "        image = cv2.rectangle(image, (625,10), (720,70), colors[2], 3)\n",
    "\n",
    "        image = cv2.rectangle(image, (40,10), (140,70), (122,122,122), -1)\n",
    "        image = cv2.rectangle(image, (160,10), (255,70), colors[0], -1)\n",
    "        image = cv2.rectangle(image, (275,10), (370,70), colors[1], -1)\n",
    "        image = cv2.rectangle(image, (390,10), (485,70), colors[2], -1)\n",
    "        image = cv2.rectangle(image, (505,10), (600,70), colors[3], -1)\n",
    "        image = cv2.rectangle(image, (625,10), (720,70), colors[4], -1)\n",
    "\n",
    "        image = cv2.line(image, (10,80),(990,80),(0,0,0),2)\n",
    "        image = cv2.putText(image, \"Clear All\", (53,45),cv2.FONT_HERSHEY_SIMPLEX,0.6,(0, 0, 0),2,cv2.LINE_AA)\n",
    "        image = cv2.cvtColor(image, cv2.COLOR_BGR2RGB)\n",
    "        image.flags.writeable = False\n",
    "        # Detections\n",
    "        results = hands.process(image)\n",
    "        # Set flag to true\n",
    "        image.flags.writeable = True\n",
    "        image = cv2.cvtColor(image, cv2.COLOR_RGB2BGR)\n",
    "        if results.multi_hand_landmarks:\n",
    "            for num, hand in enumerate(results.multi_hand_landmarks):\n",
    "                mp_drawing.draw_landmarks(image, hand, mp_hands.HAND_CONNECTIONS,\n",
    "                                          mp_drawing.DrawingSpec(color=(121, 22, 76), thickness=2, circle_radius=4),\n",
    "                                          mp_drawing.DrawingSpec(color=(250, 44, 250), thickness=2, circle_radius=2),\n",
    "                                          )\n",
    "                \n",
    "                \n",
    "                \n",
    "        if results.multi_hand_landmarks:\n",
    "            hand = results.multi_hand_landmarks\n",
    "            x1 = hand[0].landmark[8].x\n",
    "            y1 = hand[0].landmark[8].y\n",
    "            x1 = round(x1*1000)\n",
    "            y1 = round(y1*750)\n",
    "            cen = x1,y1\n",
    "            \n",
    "            # cv2.putText(image,str(x1),(34,27),cv2.FONT_HERSHEY_SIMPLEX,0.5,(0, 255, 0),1,cv2.LINE_AA) \n",
    "            if 10<=cen[1]<=75:\n",
    "                if 40<= cen[0] <=140:\n",
    "                    blue_queue = [deque()]\n",
    "                    green_queue = [deque()]\n",
    "                    black_queue = [deque()]\n",
    "                    pink_queue = [deque()]\n",
    "                    cream_queue = [deque()]\n",
    "                    blue_index = 0\n",
    "                    green_index = 0\n",
    "                    black_index = 0\n",
    "                    pink_index = 0\n",
    "                    cream_index = 0\n",
    "                    image[75:,:,:] = 255\n",
    "                elif  160<= cen[0] <=255:\n",
    "                    c = 0\n",
    "                elif  255<= cen[0] <=370:\n",
    "                    c = 1\n",
    "                elif  390<= cen[0] <=485:\n",
    "                    c = 2\n",
    "                elif  505<= cen[0] <=600:\n",
    "                    c = 3\n",
    "                elif  625<= cen[0] <=720:\n",
    "                    c = 4\n",
    "            else:\n",
    "                if c == 0:\n",
    "                    blue_queue[blue_index].appendleft(cen)\n",
    "                elif c == 1:\n",
    "                    green_queue[green_index].appendleft(cen)\n",
    "                elif c == 2:\n",
    "                    black_queue[black_index].appendleft(cen)\n",
    "                elif c == 3:\n",
    "                    pink_queue[pink_index].appendleft(cen)\n",
    "                elif c == 4:\n",
    "                    cream_queue[cream_index].appendleft(cen)\n",
    "            points = [blue_queue,green_queue,black_queue,pink_queue,cream_queue]\n",
    "            for i in range(len(points)):\n",
    "                for j in range(len(points[i])):\n",
    "                    for k in range(1, len(points[i][j])):\n",
    "                        if points[i][j][k - 1] is None or points[i][j][k] is None:\n",
    "                            continue\n",
    "                        cv2.line(image, points[i][j][k - 1], points[i][j][k], colors[i], 6)\n",
    "        cv2.imshow('Hand Tracking', image)\n",
    "\n",
    "        if cv2.waitKey(10) & 0xFF == ord('q'):\n",
    "            break\n",
    "\n",
    "cap.release()\n",
    "cv2.destroyAllWindows()"
   ],
